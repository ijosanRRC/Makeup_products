<h1>Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Product</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= link_to review.content, review_path(review) %></td>
        <td><%= review.product.name %></td>
        <td><%= review.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<footer>
<%= paginate @reviews %>
</footer>